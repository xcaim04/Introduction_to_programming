using System;

namespace TriosPitagoricos
{
    public class Program
    {
        public static void Main(string[] args)
        {
            int n, m;
            // Construir trios pitagoricos con n y m
            n = Convert.ToInt32(Console.ReadLine());
            m = Convert.ToInt32(Console.ReadLine());

            Console.WriteLine($"Trio Pitagoricos: {2*n*m} {m*2 - n*2} {m*2+n*2}");

        }
    }
}